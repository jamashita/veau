    "@jamashita/lluvia-collection": "2.1.0",
    "@jamashita/lluvia-project": "2.2.0",
    "@jamashita/lluvia-sequence": "2.2.0",
    "@jamashita/steckdose-digest": "2.0.0",
    "@jamashita/steckdose-json": "2.1.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "config": "3.3.6",
    "connect-redis": "6.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "helmet": "4.6.0",
    "http-status-codes": "2.1.4",
    "inversify": "5.1.1",
