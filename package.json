    "@jamashita/lluvia-collection": "2.11.0",
    "@jamashita/lluvia-project": "2.11.0",
    "@jamashita/lluvia-sequence": "2.11.0",
    "@jamashita/steckdose-digest": "2.3.0",
    "@jamashita/steckdose-json": "2.10.0",
    "@material-ui/core": "4.12.4",
